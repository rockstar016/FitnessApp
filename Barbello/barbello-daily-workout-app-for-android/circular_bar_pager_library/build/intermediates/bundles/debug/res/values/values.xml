<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircularViewPager"><attr format="integer" name="arc_progress"/><attr format="integer" name="progress_arc_max"/><attr format="color" name="progress_arc_clockwise_outline_color"/><attr format="color" name="progress_arc_counter_clockwise_outline_color"/><attr format="color" name="progress_arc_clockwise_color"/><attr format="color" name="progress_arc_counter_clockwise_color"/><attr format="dimension" name="progress_arc_clockwise_outline_width"/><attr format="dimension" name="progress_arc_counter_clockwise_outline_width"/><attr format="dimension" name="progress_arc_clockwise_width"/><attr format="dimension" name="progress_arc_counter_clockwise_width"/><attr format="boolean" name="progress_start_line_enabled"/><attr format="boolean" name="progress_pager_on_click_enabled"/><attr format="color" name="progress_pager_fill_circle_color"/><attr format="enum" name="progress_pager_fill_mode"><enum name="default_mode" value="0"/><enum name="pie_mode" value="1"/></attr></declare-styleable>
    <string name="app_name">library</string>
    <style name="CircularBarPager_Default">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>

        <item name="progress_arc_max">100</item>
        <item name="arc_progress">20</item>

        <item name="progress_arc_clockwise_outline_color">#00C853</item>
        <item name="progress_arc_clockwise_color">#00C853</item>
        <item name="progress_arc_counter_clockwise_outline_color">#FFFFFF</item>
        <item name="progress_arc_counter_clockwise_color">#FFFFFF</item>

        <item name="progress_arc_clockwise_outline_width">0.75dp</item>
        <item name="progress_arc_counter_clockwise_outline_width">0.75dp</item>
        <item name="progress_arc_clockwise_width">8dp</item>
        <item name="progress_arc_counter_clockwise_width">8dp</item>

        <item name="progress_pager_on_click_enabled">true</item>
    </style>
</resources>